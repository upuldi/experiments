<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

	<!-- Hibernate Mapping for domain class DailyTrades -->
<hibernate-mapping package="com.domain">

	<class name="PosNegContributors" table="EOD_DATA">

		<id name="id" column="ID" type="long">
			<generator class="native" />
		</id>

		<property name="id" column="ID" />
		<property name="positiveContributors" column="NEGATIVE_CONTRIBUTORS" />
		<property name="negativeContributors" column="POSITIVE_CONTRIBUTORS" />
		<property name="date" column="DATE" />
	</class>

</hibernate-mapping>