ALTER TABLE `upuldi_CSEDAILYDATA`.`JOBS` ADD COLUMN `STATUS` VARCHAR(45) NULL  AFTER `COMMENT` ;
ALTER TABLE `upuldi_CSEDAILYDATA`.`JOBS` ADD COLUMN `END_TIME` VARCHAR(45) NULL  AFTER `STATUS` , CHANGE COLUMN `TIME` `START_TIME` VARCHAR(45) NULL DEFAULT NULL  ;
ALTER TABLE `upuldi_CSEDAILYDATA`.`JOBS` CHANGE COLUMN `COMMENT` `COMMENT` VARCHAR(200) NULL DEFAULT NULL  ;


--SQL 18/08/2011
CREATE  TABLE `upuldi_CSEDAILYDATA`.`STOCK_TRADES` (
  `ID` INT NOT NULL AUTO_INCREMENT ,
  `DATE` VARCHAR(45) NULL ,
  `TIME` VARCHAR(45) NULL ,
  `TICKER` VARCHAR(45) NULL ,
  `PRICE` FLOAT NULL ,
  `TRADEVOL` BIGINT NULL ,
  PRIMARY KEY (`ID`) );


---sql 24/08/2011
CREATE  TABLE `upuldi_CSEDAILYDATA`.`TRADE_PROCESS` (
  `ID` INT NOT NULL AUTO_INCREMENT ,
  `DATE` VARCHAR(45) NULL ,
  `TICKER` VARCHAR(45) NULL ,
  `PROCESSED` VARCHAR(45) NULL ,
  PRIMARY KEY (`ID`) );


--sql 25/08/2011
ALTER TABLE `upuldi_CSEDAILYDATA`.`TRADE_PROCESS` ADD COLUMN `INFO` VARCHAR(100) NULL  AFTER `PROCESSED` ;

---SQL 02/11/2011
CREATE  TABLE `upuldi_CSEDAILYDATA`.`CSE_LISTED_COMPANIES` (
  `ID` INT NOT NULL AUTO_INCREMENT ,
  `NAME` VARCHAR(45) NULL ,
  `TICKER` VARCHAR(45) NULL ,
  `CSE_ID` INT NULL ,
  PRIMARY KEY (`ID`) );
