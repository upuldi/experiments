CREATE  TABLE `upuldi_CSEDAILYDATA`.`GOLD` (
  `ID` INT NOT NULL AUTO_INCREMENT ,
  `Date` VARCHAR(45) NULL ,
  `GoldOunce` VARCHAR(45) NULL ,
  `GoldPound` VARCHAR(45) NULL ,
  `GramKarat24` VARCHAR(45) NULL ,
  `GramKarat22` VARCHAR(45) NULL ,
  `GramKarat21` VARCHAR(45) NULL ,
  `GramKarat18` VARCHAR(45) NULL ,
  `GramKarat14` VARCHAR(45) NULL ,
  PRIMARY KEY (`ID`) );

ALTER TABLE `upuldi_CSEDAILYDATA`.`GOLD` CHANGE COLUMN `Date` `Date` DATE NULL DEFAULT NULL  ;